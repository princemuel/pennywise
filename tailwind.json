{
  "theme": {
    "colors": {
      "beige": {
        "100": "#f8f4f0",
        "500": "#98908b"
      },
      "grey": {
        "100": "#f2f2f2",
        "300": "#b3b3b3",
        "500": "#696868",
        "900": "#201f24"
      },
      "accent": {
        "100": "#f2cdac",
        "200": "#c94736",
        "300": "#82c9d7",
        "400": "#277c78",
        "500": "#826cb0",
        "600": "#626070"
      },
      "brand": {
        "100": "#be6c49",
        "200": "#9367af",
        "300": "#cab361",
        "400": "#7f9161",
        "500": "#597c7c",
        "600": "#97a0ac",
        "700": "#3f82b2",
        "800": "#af81ba",
        "900": "#934f6f"
      }
    },
    "fontFamily": {
      "sans": "\"Public Sans Variable\""
    },
    "cursor": {
      "pointer": "url(\"data:image/png;base64,\"), pointer;"
    },
    "animation": {
      "bounce": "bounce 5s ease-in-out infinite alternate",
      "float": "float 6s ease-in-out infinite",
      "rotate": "rotate 1s ease-out",
      "loading": "skeleton 1s linear infinite alternate"
    },
    "keyframes": {
      "bounce": {
        "0%": { "transform": "translate(0, 0)" },
        "25%": { "transform": "translate(10px, -10px)" },
        "50%": { "transform": "translate(-10px, 10px)" },
        "75%": { "transform": "translate(10px, 10px)" },
        "100%": { "transform": "translate(-10px, -10px)" }
      },
      "float": {
        "0%, 100%": { "transform": "translate3d(0px, -8px, 0)" },
        "50%": { "transform": "translate3d(0px, 8px, 0)" }
      },
      "rotate": {
        "0%": { "transform": "rotate(-45deg)" },
        "100%": { "transform": "rotate(0deg)" }
      },
      "skeleton": {
        "0%": { "background-color": "hsl(200, 20%, 80%)" },
        "100%": { "background-color": "hsl(200, 20%, 95%)" }
      }
    }
  }
}
